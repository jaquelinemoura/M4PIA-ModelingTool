      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="GeneratedFile Diagram" domainClass="M4PIAmetamodel::GeneratedFile" enablePopupBars="true">
      <metamodel href="http://www.das.ufsc.br/M4PIAmetamodel#/"/>
      <layout xsi:type="description_1:CompositeLayout" direction="LeftToRight"/>
      <defaultLayer name="Default">
        <nodeMappings name="Equipment" semanticCandidatesExpression="feature:entity" domainClass="M4PIAmetamodel::Equipment">
          <style xsi:type="style:BundledImageDescription" labelSize="12" showIcon="false" resizeKind="NSEW" shape="triangle">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='dark_blue']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="EquipmentInheritance" semanticCandidatesExpression="aql:self.entity.siblings().equipment.base" sourceMapping="//@ownedViewpoints[name='tool']/@ownedRepresentations[name='GeneratedFile%20Diagram']/@defaultLayer/@nodeMappings[name='Equipment'] //@ownedViewpoints[name='tool']/@ownedRepresentations[name='Project%20Diagram']/@defaultLayer/@nodeMappings[name='GeneratedFile']" targetMapping="//@ownedViewpoints[name='tool']/@ownedRepresentations[name='GeneratedFile%20Diagram']/@defaultLayer/@nodeMappings[name='Equipment']" targetFinderExpression="feature:base" sourceFinderExpression="feature:base" domainClass="M4PIAmetamodel::Equipment" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="Relationship" semanticCandidatesExpression="aql:self.eAllContents()" sourceMapping="//@ownedViewpoints[name='tool']/@ownedRepresentations[name='GeneratedFile%20Diagram']/@defaultLayer/@nodeMappings[name='Equipment']" targetMapping="//@ownedViewpoints[name='tool']/@ownedRepresentations[name='GeneratedFile%20Diagram']/@defaultLayer/@nodeMappings[name='Equipment']" targetFinderExpression="feature:base" sourceFinderExpression="feature:base" domainClass="M4PIAmetamodel::Equipment" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="m4pia.design.Services"/>
  </ownedViewpoints>
  <extensions xsi:type="properties:ViewExtensionDescription" name="Properties">
    <categories name="Default Category">
      <pages name="Default Page" labelExpression="Page" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0"/>
      <groups name="Default Group" labelExpression="Group" semanticCandidateExpression="var:self"/>
    </categories>
  </extensions>
</description:Group>
